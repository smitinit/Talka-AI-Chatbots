{
  "name": "@qb/chatbot-ui",
  "version": "0.1.0",
  "description": "Secure, verified, embeddable chatbot widget with modern UI",
  "author": "QB",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc && npm run copy-css",
    "copy-css": "copyfiles -u 1 src/**/*.css dist/"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "chatbot",
    "react",
    "typescript",
    "ui",
    "widget",
    "secure",
    "embeddable"
  ],
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "emoji-picker-react": "^4.15.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.453.0",
    "next": "^16.0.1",
    "react": ">=18",
    "react-dom": ">=18",
    "react-markdown": "^9.1.0",
    "react-textarea-autosize": "^8.5.9",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.10",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "sideEffects": [
    "**/*.css"
  ]
}
